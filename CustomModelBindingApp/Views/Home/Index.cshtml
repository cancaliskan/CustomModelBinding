        var dataType = 'application/x-www-form-urlencoded; charset=utf-8';
        var data = $('form').serialize();

        //JSON data
        var dataType = 'application/json; charset=utf-8';

        console.log('Submitting form...');
        $.ajax({
            type: 'POST',
            url: '/Home/Index',
            dataType: 'json',
            contentType: dataType,
            data: GetModelData(),
            success: function (result) {
                alert(result);
