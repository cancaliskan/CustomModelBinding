@model List<UserModel>
<form action="/" method="post">
    <table>
        <thead>
            <tr>
                <th></th>
                <th>User Name</th>
                <th>User Address</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var person in Model)
            {
                <tr>
                    <th>
                        <input type="checkbox" name="UserUid" id=@person.UserIid value=@string.Concat(@person.UserIid,"|",@person.Name,"|",@person.Address,"^") />
                    </th>
                    <th><label for=@person.Name>@person.Name </label></th>
                    <th><label for=@person.Address>@person.Address </label></th>
                </tr>
            }
        </tbody>
    </table>


    <div>
        <button onclick="Next()" id="submit" type="button">Next</button>
    </div>
</form>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>

